<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record model="ir.ui.view" id="ia_view_stock_quant_tree_inherit">
        <field name="name">ia.stock.quant.tree.inherit</field>
        <field name="model">stock.quant</field>
        <field name="inherit_id" ref="stock.view_stock_quant_tree"></field>
        <field name="priority">15</field>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='product_uom_id']" position="after">               
                <field name="product_value"/>
            </xpath>
        </field>
    </record>

    <record model="ir.ui.view" id="ia_view_stock_quant_tree_editable_inherit">
        <field name="name">ia.stock.quant.tree.editable.inherit</field>
        <field name="model">stock.quant</field>
        <field name="inherit_id" ref="stock.view_stock_quant_tree_editable"></field>
         <field name="priority">15</field>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='product_uom_id']" position="after">             
                <field name="product_value"/>
            </xpath>
        </field>
    </record>

</odoo>
